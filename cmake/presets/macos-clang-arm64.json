{
  "version": 6,
  "include": [
    "base.json"
  ],
  "configurePresets": [
    {
      "name": "macos-clang-arm64-dbg",
      "displayName": "MacOS Clang ARM64 Debug",
      "inherits": "macos-clang-base",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_OSX_ARCHITECTURES":"arm64"
      }
    },
    {
      "name": "macos-clang-arm64-rel",
      "displayName": "MacOS Clang ARM64 Release",
      "inherits": "macos-clang-base",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_OSX_ARCHITECTURES":"arm64"
      }
    }
  ]
}
