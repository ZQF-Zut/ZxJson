        "CMAKE_EXPORT_COMPILE_COMMANDS": true,
        "CMAKE_CATCH_DISCOVER_TESTS_DISCOVERY_MODE": "PRE_TEST",
        "VCPKG_INSTALL_OPTIONS": "--no-print-usage",
        "ZQF_DEVELOP": true,
        "ZQF_TESTING": true,
        "ZQF_DOXYGEN": true,
        "ZQF_VCPKG": true
      }
    },
    {
      "name": "mingw-gcc-x64-dbg",
      "displayName": "MinGW GCC x64 Debug",
      "inherits": "mingw-gcc-base",
