        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Darwin"
      }
    },
    {
      "name": "mingw-gcc-base",
      "inherits": "config-base",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++"
      },
      "condition": {
        "type": "anyOf",
        "conditions": [
          {
            "type": "equals",
            "lhs": "$env{MSYSTEM}",
            "rhs": "MINGW32"
          },
          {
            "type": "equals",
            "lhs": "$env{MSYSTEM}",
            "rhs": "MINGW64"
          }
        ]
      }
